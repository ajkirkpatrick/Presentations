@article{congUnveilingHiddenEnergy2022,
  title = {Unveiling Hidden Energy Poverty Using the Energy Equity Gap},
  author = {Cong, Shuchen and Nock, Destenie and Qiu, Yueming Lucy and Xing, Bo},
  date = {2022-05-04},
  journaltitle = {Nature Communications 2022 13:1},
  volume = {13},
  number = {1},
  pages = {1--12},
  publisher = {Nature Publishing Group},
  issn = {2041-1723},
  doi = {10.1038/s41467-022-30146-5},
  url = {https://www.nature.com/articles/s41467-022-30146-5},
  urldate = {2022-05-04},
  abstract = {Income-based energy poverty metrics ignore people’s behavior patterns, particularly reducing energy consumption to limit financial stress. We investigate energy-limiting behavior in low-income households using a residential electricity consumption dataset. We first determine the outdoor temperature at which households start using cooling systems, the inflection temperature. Our relative energy poverty metric, the energy equity gap, is defined as the difference in the inflection temperatures between low and high-income groups. In our study region, we estimate the energy equity gap to be between 4.7–7.5\,°F (2.6–4.2\,°C). Within a sample of 4577 households, we found 86 energy-poor and 214 energy-insecure households. In contrast, the income-based energy poverty metric, energy burden (10\% threshold), identified 141 households as energy-insecure. Only three households overlap between our energy equity gap and the income-based measure. Thus, the energy equity gap reveals a hidden but complementary aspect of energy poverty and insecurity. In the summer, low-income households in the Arizona,\&nbsp;US wait 4 - 7\,°F (2.6–4.2\,°C) longer than high-income households to turn on their AC units to save money on energy bills. This energy limiting behavior indicates a hidden form of energy poverty.},
  keywords = {Energy and society,Energy justice,Energy supply and demand},
  file = {C:\Users\jkirk\Zotero\storage\Q9C36QYD\full-text.pdf}
}

@article{Conley2011,
  title = {Inference with “{{Difference}} in {{Differences}}” with a {{Small Number}} of {{Policy Changes}}},
  author = {Conley, Timothy G. and Taber, Christopher R.},
  date = {2011-02-21},
  journaltitle = {Review of Economics and Statistics},
  volume = {93},
  number = {1},
  pages = {113--125},
  publisher = {The MIT Press},
  issn = {0034-6535},
  doi = {10.1162/REST_a_00049},
  url = {http://www.mitpressjournals.org/doi/abs/10.1162/REST_a_00049#.U5M9a5RdXv0},
  urldate = {2014-06-07},
  abstract = {Abstract In difference-in-differences applications, identification of the key parameter often arises from changes in policy by a small number of groups. In contrast, typical inference assumes that the number of groups changing policy is large. We present an alternative inference approach for a small (finite) number of policy changers, using information from a large sample of nonchanging groups. Treatment effect point estimators are not consistent, but we can consistently estimate their asymptotic distribution under any point null hypothesis about the treatment. Thus, treatment point estimators can be used as test statistics, and confidence intervals can be constructed using test statistic inversion.},
  langid = {english}
}

@article{considineEffectivenessHomeEnergy2016,
  title = {The Effectiveness of Home Energy Audits: {{A}} Case Study of {{Jackson}}, {{Wyoming}}},
  author = {Considine, Timothy J and Sapci, Onur},
  date = {2016},
  journaltitle = {Resource and Energy Economics},
  volume = {44},
